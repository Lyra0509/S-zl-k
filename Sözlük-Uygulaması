None
meme_dict = {
            "CRINGE": "Garip ya da utandırıcı bir şey",
            "LOL": "Komik bir şeye verilen cevap",
            "WDYM":"Ne demek istiyorsun",
            "P2W":"Öde ve kazan",
            "BTW":"Bu arada",
            "GG": "İyi Oyundu",
            "IDK":"Bilmiyorum",
            "AFK": "Ekran başında değilim",
            "BRB": "be right back",
            }
while True:
    word = input("Sormak istediğiniz kelimeyi yazınız").upper()
    if word in meme_dict:
        print(meme_dict[word])
    else:
        print("Aradığınız kelime burada yok")
